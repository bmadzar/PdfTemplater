<?xml version="1.0" encoding="UTF-8" ?>

<!-- PDF Templater XML DTD v1.0 -->

<!ELEMENT Document (Page+)>
<!ATTLIST Document author CDATA #IMPLIED>
<!ATTLIST Document title CDATA #REQUIRED>
<!ATTLIST Document keywords CDATA #IMPLIED>
<!ATTLIST Document filename CDATA #IMPLIED>
<!ATTLIST Document description CDATA #IMPLIED>
<!ATTLIST Document id ID #IMPLIED>

<!ELEMENT Page (Bookmark | Ellipse | Image | Line | Rectangle | Text)*>
<!ATTLIST Page height CDATA #REQUIRED>
<!ATTLIST Page width CDATA #REQUIRED>
<!ATTLIST Page number CDATA #REQUIRED>
<!ATTLIST Page id ID #IMPLIED>

<!ELEMENT Bookmark (EMPTY)>
<!-- Standard Element attributes -->
<!ATTLIST Bookmark height CDATA #IMPLIED>
<!ATTLIST Bookmark width CDATA #IMPLIED>
<!ATTLIST Bookmark top CDATA #IMPLIED>
<!ATTLIST Bookmark right CDATA #IMPLIED>
<!ATTLIST Bookmark bottom CDATA #IMPLIED>
<!ATTLIST Bookmark left CDATA #IMPLIED>
<!ATTLIST Bookmark height-rel IDREF #IMPLIED>
<!ATTLIST Bookmark width-rel IDREF #IMPLIED>
<!ATTLIST Bookmark top-rel IDREF #IMPLIED>
<!ATTLIST Bookmark right-rel IDREF #IMPLIED>
<!ATTLIST Bookmark bottom-rel IDREF #IMPLIED>
<!ATTLIST Bookmark left-rel IDREF #IMPLIED>
<!ATTLIST Bookmark layer CDATA #IMPLIED>
<!ATTLIST Bookmark id ID #IMPLIED>
<!-- Other attributes -->
<!ATTLIST Bookmark name CDATA #REQUIRED>
<!ATTLIST Bookmark level CDATA #IMPLIED "0">

<!ELEMENT Ellipse (EMPTY)>
<!-- Standard Element attributes -->
<!ATTLIST Ellipse height CDATA #IMPLIED>
<!ATTLIST Ellipse width CDATA #IMPLIED>
